setcollision(2)

decl alive
alive=3

decl ctr
ctr = 0

setsprite(160)
setcol(3, 3, 0)

while ( alive > 0 ) {

    ctr = ctr + 1

    if ( ctr < 60 ) { setvel(0-1, 1) }
    if ( ctr > 60 ) { setvel(1, 0-1) }
    if ( ctr > 120 ) { ctr = 0 }

    if ( getcollision(3)  )
    {
        alive = alive - 1
    }

}

